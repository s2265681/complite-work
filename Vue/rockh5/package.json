      "indent_size": "2",
      "end_of_line": "lf",
      "insert_final_newline": "true",
      "trim_trailing_whitespace": "true"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Android >= 4.4",
    "iOS >= 9"
  ]
