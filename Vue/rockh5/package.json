{
  "name": "rockh5",
  "version": "0.1.0",
  "private": false,
  "description": "use vue make h5 component",
  "author": "商佳伟 - rock shang",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name rockh5 src/rockh5.ts",
